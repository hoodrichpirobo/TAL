Cell[TextData[{
 "Implement a ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " module whose input is a word x and an integer k. The module should return \
the set of k-length segments of x. "
}], "ItemNumbered",
 CellChangeTimes->{
  3.587281372564268*^9, {3.58728148594456*^9, 3.587281550798246*^9}, {
   3.587283510258602*^9, 3.587283510357462*^9}, {3.587796072665926*^9, 
   3.587796153058509*^9}},ExpressionUUID->"e1bd4364-5e4f-4b33-af0a-\
e9051d1e1674"],

Cell[TextData[StyleBox["Hint: The last position of x where it is possible to \
find a segment of length k is position n-k+1, where n denotes the length of \
the word.",
 FontSize->11]], "Text",
 CellChangeTimes->{{3.587281606767194*^9, 3.587281675346916*^9}, {
  3.587281707489728*^9, 3.587281913367154*^9}, {3.5877961549608593`*^9, 
  3.587796219659649*^9}},ExpressionUUID->"fc7fe831-61df-4d4e-8b1d-\
f1355343f6d4"],

Cell[BoxData[
 RowBox[{
  RowBox[{"KSegment", "[", 
   RowBox[{"x_", ",", " ", "k_"}], "]"}], " ", ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"i", ",", " ", "ans"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"ans", " ", "=", " ", 
      RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", " ", "=", " ", "1"}], ",", " ", 
       RowBox[{"i", " ", "\[LessEqual]", " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"Length", "[", "x", "]"}], " ", "-", " ", "k", " ", "+", 
          " ", "1"}], ")"}]}], ",", " ", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"AppendTo", "[", 
         RowBox[{"ans", ",", " ", 
          RowBox[{"Take", "[", 
           RowBox[{"x", ",", " ", 
            RowBox[{"{", 
             RowBox[{"i", ",", " ", 
              RowBox[{"i", " ", "+", " ", "k", " ", "-", " ", "1"}]}], 
             "}"}]}], "]"}]}], "]"}], ";"}]}], "\[IndentingNewLine]", "]"}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "ans", "]"}], ";"}]}], "\[IndentingNewLine]", 
   "]"}], 
  RowBox[{"(*", " ", 
   RowBox[{"K", " ", "Segment", " ", "or", " ", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"Take", "[", 
       RowBox[{"x", ",", " ", 
        RowBox[{"{", 
         RowBox[{"i", ",", " ", 
          RowBox[{"i", " ", "+", " ", "k", " ", "-", " ", "1"}]}], "}"}]}], 
       "]"}], ",", " ", 
      RowBox[{"{", 
       RowBox[{"i", ",", " ", "1", ",", " ", 
        RowBox[{
         RowBox[{"Length", "[", "x", "]"}], " ", "-", " ", "k", " ", "+", " ",
          "1"}]}], "}"}]}], "]"}]}], " ", "*)"}]}]], "Input",
