Cell[TextData[{
 "Implement a ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " module that, given a list and two integers i and j, returns the list with \
the elements in positions i and j swapped."
}], "ItemNumbered",
 CellChangeTimes->{{3.587282722409523*^9, 3.587282769495165*^9}, {
   3.587795676248272*^9, 3.5877957322055607`*^9}, 
   3.587796445877137*^9},ExpressionUUID->"96da6859-223c-403e-a520-\
51b13109ece1"],

Cell[TextData[StyleBox["Hint: Remember that the elements of a list l can be \
indexed in the form l[[i]].",
 FontSize->11]], "Text",
 CellChangeTimes->{{3.587282822392935*^9, 3.587282855467697*^9}, {
  3.587795745458213*^9, 
  3.587795781277614*^9}},ExpressionUUID->"81cd7a07-fdc0-4d30-81a5-\
435525553b83"],

Cell[BoxData[
 RowBox[{
  RowBox[{"Swap", "[", 
   RowBox[{"list_", ",", " ", "i_", ",", " ", "j_"}], "]"}], " ", ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "ans", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"ans", "=", " ", "list"}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"ans", "[", 
       RowBox[{"[", "i", "]"}], "]"}], " ", "=", " ", 
      RowBox[{"list", "[", 
       RowBox[{"[", "j", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"ans", "[", 
       RowBox[{"[", "j", "]"}], "]"}], " ", "=", " ", 
      RowBox[{"list", "[", 
       RowBox[{"[", "i", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "ans", "]"}], ";"}]}], "\[IndentingNewLine]", 
   "]"}], " ", 
  RowBox[{"(*", " ", "Swap", " ", "*)"}]}]], "Input",
 CellChangeTimes->{{3.943624191673456*^9, 3.9436248039910936`*^9}},
 CellLabel->"In[20]:=",ExpressionUUID->"b4e4af25-9c94-4340-adbb-9fc160a3a960"],

Cell[TextData[{
 "Implement a ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " module whose input is a list and an element, and whose output is the \
number of occurrences of the element in the list."
}], "ItemNumbered",
 CellChangeTimes->{{3.587281168338705*^9, 3.587281237081118*^9}, {
