  3.5877958349416437`*^9}},ExpressionUUID->"19115b19-0dd7-448e-ac77-\
cfc74f58128d"],

Cell[TextData[StyleBox["Algorithm: Initialize a counter to zero. Traverse the \
list and compare each element with the input one. Increase the counter in \
case both are equal.",
 FontSize->11]], "Text",
 CellChangeTimes->{{3.587281606767194*^9, 3.587281675346916*^9}, 
   3.5872819182482033`*^9, {3.587795849779948*^9, 3.5877959129150743`*^9}, {
   3.943625028247251*^9, 
   3.9436250337499084`*^9}},ExpressionUUID->"b3b631d8-62c8-4004-a1ab-\
d90e6147bb70"],

Cell[BoxData[
 RowBox[{
  RowBox[{"Ocurrences", "[", 
   RowBox[{"list_", ",", " ", "e_"}], "]"}], " ", ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"i", ",", " ", "ans"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"ans", " ", "=", " ", "0"}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", " ", "=", " ", "1"}], ",", " ", 
       RowBox[{"i", " ", "\[LessEqual]", " ", 
        RowBox[{"Length", "[", "list", "]"}]}], ",", " ", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"list", "[", 
            RowBox[{"[", "i", "]"}], "]"}], " ", "\[Equal]", " ", "e"}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"ans", "++"}], ";"}]}], "\[IndentingNewLine]", "]"}], 
        ";"}]}], "\[IndentingNewLine]", "]"}], ";"}]}], "\[IndentingNewLine]",
    "]"}], " ", 
  RowBox[{"(*", " ", 
   RowBox[{"Ocurrences", ",", " ", 
    RowBox[{
     RowBox[{"although", " ", 
      RowBox[{"there", "'"}], "s", " ", "already", " ", "a", " ", "Count", 
      " ", "module", " ", "built"}], "-", "in"}]}], " ", "*)"}]}]], "Input",
 CellChangeTimes->{{3.943625043542988*^9, 3.9436250489659653`*^9}, {
