Cell[TextData[{
 "Implement a ",
 StyleBox["Mathematica",
  FontSlant->"Italic"],
 " module that returns the set of prefixes of an input  word x."
}], "ItemNumbered",
 CellChangeTimes->{{3.587281339099203*^9, 3.5872813668169413`*^9}, {
  3.587283513203683*^9, 3.5872835133160753`*^9}, {3.5877959219156723`*^9, 
  3.587795957656211*^9}},ExpressionUUID->"9ec79b82-7d37-4167-acc2-\
d775b486479a"],

Cell[TextData[StyleBox["Algorithm: Initialize the output list with the empty \
list. For i equal to zero to the length of the list, obtain the prefix of \
length i using the built-in command Take. Add this prefix to the output list.",
 FontSize->11]], "Text",
 CellChangeTimes->{{3.587281606767194*^9, 3.587281675346916*^9}, {
   3.587281707489728*^9, 3.587281829710855*^9}, 3.587281916380336*^9, {
   3.587795961245153*^9, 3.587796069595524*^9}, {3.587796449781389*^9, 
   3.587796455720978*^9}},ExpressionUUID->"08abba12-30f3-441a-9077-\
54c7f5ec279f"],

Cell[BoxData[
 RowBox[{
  RowBox[{"Prefixes", "[", "word_", "]"}], " ", ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"ans", ",", " ", "i"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"ans", " ", "=", " ", 
      RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", " ", "=", " ", "0"}], ",", " ", 
       RowBox[{"i", " ", "<=", " ", 
        RowBox[{"Length", "[", "word", "]"}]}], ",", " ", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"AppendTo", "[", 
         RowBox[{"ans", ",", " ", 
          RowBox[{"Take", "[", 
           RowBox[{"word", ",", " ", "i"}], "]"}]}], "]"}], ";"}]}], 
      "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "ans", "]"}], ";"}]}], "\[IndentingNewLine]", 
   "]"}], " ", 
  RowBox[{"(*", " ", 
   RowBox[{"Prefixes", " ", "or", " ", "also", " ", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"Take", "[", 
       RowBox[{"word", ",", " ", "i"}], "]"}], ",", " ", 
      RowBox[{"{", 
       RowBox[{"i", ",", " ", "0", ",", " ", 
        RowBox[{"Length", "[", "word", "]"}]}], "}"}]}], "]"}]}], " ", 
   "*)"}]}]], "Input",
 CellChangeTimes->{{3.9436258291844487`*^9, 3.9436261280756865`*^9}, {
   3.943626236356653*^9, 3.94362636776331*^9}, 3.943626459147895*^9, {
